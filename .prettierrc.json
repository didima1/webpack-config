{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": false,
  "printWidth": 130,
  "tabWidth": 3,
  "embeddedLanguageFormatting": "auto",
  "singleQuote": true,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "singleAttributePerLine": true,
  "quoteProps": "preserve",
  "arrowParens": "avoid",
  "experimentalTernaries": true
}
